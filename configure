EOF

      cat >> confdefs.h <<\EOF
#define XASTIR_SYSTEM "Darwin"
EOF

      ac_tnc_port='"/dev/cua/a"'
      ac_gps_port='"/dev/cua/b"'
      echo "configure: warning: *** Mac OS X support is not fully implemented yet! ***" 1>&2
      ;;
   * )
      echo "configure: warning: Host type is not recognized" 1>&2
      ac_tnc_port='""'
      ac_gps_port='""'
      ;;
esac

