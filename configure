      ;;
   *-*-*linux* )
      cat >> confdefs.h <<\EOF
#define XASTIR_SYSTEM "Linux"
EOF

      ac_tnc_port='"/dev/ttyS0"'
      ac_gps_port='"/dev/ttyS1"'
      ;;
    *-*apple-darwin* )
      cat >> confdefs.h <<EOF
